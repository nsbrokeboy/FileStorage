import cv2

# Загружаем фотографию
image = cv2.imread('img.jpg')

# Создаем объект CascadeClassifier для использования предварительно обученной модели
face_cascade = cv2.CascadeClassifier('haarcascade_frontalface_default.xml')

# Преобразуем фотографию в черно-белое изображение
gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)

# Ищем лица на фотографии
faces = face_cascade.detectMultiScale(gray, 1.1, 4)

# Отмечаем найденные лица на фотографии
for (x, y, w, h) in faces:
    cv2.rectangle(image, (x, y), (x+w, y+h), (0, 255, 0), 2)

# Отображаем фотографию с выделенными лицами
cv2.imshow('image', image)
cv2.waitKey(0)